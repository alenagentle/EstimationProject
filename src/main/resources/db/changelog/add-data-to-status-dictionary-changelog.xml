<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
        http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.1.xsd">

    <changeSet author="kiseleva_ev" id="add-data-to-status_dictionary">
        <insert  tableName="status_dictionary">
            <column  name="value"  value="draft"/>
            <column  name="display_value"  value="Черновик"/>
        </insert>
        <insert  tableName="status_dictionary">
            <column  name="value"  value="in_progress"/>
            <column  name="display_value"  value="В работе"/>
        </insert>
        <insert  tableName="status_dictionary">
            <column  name="value"  value="done"/>
            <column  name="display_value"  value="Завершен"/>
        </insert>
    </changeSet>

</databaseChangeLog>
